+\gdef\FOaddmarker#1#2{%
+ \ifx\relax#1
+ \else
+   \def\FOtemp{#1}%
+   \ifx\FOtemp\FOmarkerclassname
+   \else
+        \toks@\expandafter{\the\toks@{#1}{#2}}%
+   \fi
+   \expandafter\FOaddmarker
+ \fi}
