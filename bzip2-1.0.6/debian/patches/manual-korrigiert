+\define@key{Gin}{scale}{%
+  \if@tempswa
+    \edef\@tempa{\toks@{\noexpand\Gscale@box{#1}[#1]{\the\toks@}}}%
+    \@tempa
+  \else
+    \def\Gin@req@sizes{%
+      \def\Gin@scalex{#1}\let\Gin@scaley\Gin@exclamation
+      \Gin@req@height\Gin@scalex\Gin@nat@height
+      \Gin@req@width\Gin@scalex\Gin@nat@width}%
+  \fi
+  \@tempswatrue}
