+% support .gif and .eps
+\g@addto@macro\Gin@extensions{,.eps}
+\g@addto@macro\Gin@extensions{,.gif}
+\@namedef{Gin@rule@.eps}#1{{pdf}{.pdf}{`epstopdf #1}}
+\@namedef{Gin@rule@.gif}#1{{png}{.png}{`giftopng #1}}
+
+\endinput
